{
    "name": "PaperArxiv",
    "version": "0.1.1",
    "description": "A paper management tool",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "test": "ava",
        "buildlinux": "electron-packager .  PaperArxiv --platform=linux --arch=x64 --out dist/ --asar --overwrite",
        "buildwin": "electron-packager .  PaperArxiv --platform=win32 --arch=x64 --out dist/ --icon=icons/pa.ico --prune=true --asar --overwrite ",
        "buildmac": "electron-packager .  PaperArxiv --platform=darwin --arch=x64 --out dist/ --icon=icons/pa.ico --asar --overwrite ",
        "deb64": "electron-installer-debian --src dist/PaperArxiv-linux-x64 --dest dist/installers/ --icon icons/pa.icns --arch amd64",
        "win64": "node buildwin64.js",
        "buildall": "electron-packager .  PaperArxiv --platform=all --arch=x64,ia32 --out dist/ --icon icons/pa --asar --overwrite"
    },
    "postinstall": "./node_modules/.bin/electron-rebuild",
    "repository": "https://github.com/electron/electron-quick-start",
    "keywords": [
        "Electron",
        "quick",
        "start",
        "tutorial",
        "demo"
    ],
    "author": "Maple",
    "license": "MIT",
    "devDependencies": {
        "ava": "0.22.0",
        "electron": "16.0.5",
        "electron-builder": "^19.27.2",
        "electron-installer-debian": "3.1.0",
        "electron-packager": "15.4.0",
        "electron-rebuild": "3.2.5",
        "electron-winstaller": "2.6.3",
        "vue-devtools": "3.1.6"
    },
    "engines": {
        "node": "14.18.2"
    },
    "dependencies": {
        "@liradb2000/markdown-it-mermaid": "^0.4.4",
        "@shd101wyy/mume": "^0.6.9",
        "@traptitech/markdown-it-katex": "^3.5.0",
        "better-sqlite3": "^7.4.5",
        "child_process": "1.0.2",
        "element-ui": "2.15.6",
        "esm": "^3.2.25",
        "file-dialog": "0.0.8",
        "home-dir": "1.0.0",
        "katex": "0.15.1",
        "markdown-it": "^12.3.0",
        "node-poppler": "^5.1.1",
        "opn": "5.4.0",
        "path": "0.12.7",
        "split.js": "1.6.4",
        "string-similarity": "3.0.0",
        "title-case": "3.0.3",
        "v-lazy-component": "2.0.0",
        "vue": "2.6.14",
        "xmldom": "0.1.27",
        "xpath": "0.0.24"
    }
}
